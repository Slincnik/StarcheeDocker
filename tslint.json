{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-unused-expression": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-console": false,
        "no-shadowed-variable": false
    },
    "rulesDirectory": []
}
